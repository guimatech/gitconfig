[user]
	name = [your name]
	email = [your email]
[core]
	editor = code --wait
[difftool]
	editor = code --wait
[alias]
	s = !git status -s
	c = !git add --all && git commit -m
	l = !git log --pretty=format:'%C(blue)%h%C(red)%d %C(white)%s - %C(cyan)%cn, %C(green)%cr'
	ck = !git checkout
[credential]
	helper = manager
